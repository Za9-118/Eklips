def on_ready(empty):
    return

def _process(empty):
    if ord("w") in keys_pressed:
        myself.parameters["transform"]["pos"][1] += 5 
    if ord("a") in keys_pressed:
        myself.parameters["transform"]["pos"][0] += 5
    if ord("s") in keys_pressed:
        myself.parameters["transform"]["pos"][1] -= 5
    if ord("d") in keys_pressed:
        myself.parameters["transform"]["pos"][0] -= 5